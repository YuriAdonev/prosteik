mixin icon(name,mod)
	if(mod)
		svg(class="svg-sprite-icon icon-" + name + ' ' + mod)
			use(xlink:href="static/images/svg/symbol/sprite.svg#" + name)
	else
		svg(class="svg-sprite-icon icon-" + name)
			use(xlink:href="static/images/svg/symbol/sprite.svg#" + name)
			
mixin sectionHeader(sectionHeaderTitle, sectionHeaderSubtitle)
	.section-header
		.section-header__title
			.section-header__title-bg
			span= sectionHeaderTitle
		.section-header__subtitle-wrap
			.section-header__subtitle= sectionHeaderSubtitle
		